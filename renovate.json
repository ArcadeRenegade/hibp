{
  "extends": ["config:js-lib", ":semanticCommits"],
  "commitMessageTopic": "{{depName}}",
  "automergeType": "branch",
  "automerge": true,
  "major": {
    "automerge": false
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "eslint",
        "@types/node-fetch",
        "@types/node",
        "node"
      ],
      "matchUpdateTypes": ["major"],
      "enabled": false
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
