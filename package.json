{
  "name": "hibp",
  "version": "2.1.0",
  "description": "A Promise-based API for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "lib/hibp.js",
  "browser": "lib/hibp.min.js",
  "scripts": {
    "clean": "rimraf ./lib ./coverage ./nyc_output",
    "compile": "babel src --out-dir lib --source-maps && npm run webpack",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run compile",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha",
    "webpack": "webpack -p"
  },
  "nyc": {
    "all": true,
    "include": [
      "**/src/**"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://github.com/wKovacs64/hibp",
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.1",
    "es6-promise": "^3.2.1",
    "eslint": "^3.4.0",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "moxios": "^0.3.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2"
  }
}
