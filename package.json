{
  "name": "hibp",
  "version": "1.0.6-dev.0",
  "description": "A Node.js module for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "lib/hibp.js",
  "scripts": {
    "clean": "rm -rf ./lib ./coverage",
    "compile": "npm run clean && babel src --out-dir lib --source-maps",
    "coverage": "npm run compile && istanbul cover node_modules/mocha/bin/_mocha -- -R spec lib/test",
    "prepublish": "npm run compile",
    "test": "eslint src test && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://github.com/wKovacs64/hibp",
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "node-fetch": "^1.5.3",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "es6-promise": "^3.2.1",
    "eslint": "^2.12.0",
    "eslint-config-semistandard": "^6.0.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "expect.js": "^0.3.1",
    "fetch-mock": "^4.5.4",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0"
  }
}
