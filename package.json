{
  "name": "hibp",
  "version": "3.0.1-dev.0",
  "description": "A Promise-based API for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "lib/hibp.js",
  "browser": "dist/hibp.min.js",
  "scripts": {
    "build": "npm run compile",
    "clean": "rimraf ./dist ./lib ./coverage ./nyc_output",
    "compile": "babel src --out-dir lib --source-maps",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run compile && npm run webpack",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha",
    "webpack": "npm run webpack:dev && npm run webpack:prod",
    "webpack:dev": "webpack --progress --config webpack/webpack.config.dev.js",
    "webpack:prod": "webpack -p --progress --config webpack/webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://wkovacs64.github.io/hibp",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "source-map-support": "^0.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "mocha": "^3.1.2",
    "moxios": "^0.3.0",
    "nyc": "^9.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3"
  }
}
