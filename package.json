{
  "name": "hibp",
  "version": "1.0.8-dev.0",
  "description": "A Node.js module for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "lib/hibp.js",
  "scripts": {
    "clean": "npm run srcClean && npm run testClean",
    "compile": "npm run clean && npm run srcCompile && npm run testCompile",
    "coverage": "npm run compile && istanbul cover -x \"**/lib-test/**\" node_modules/mocha/bin/_mocha -- -R spec lib-test",
    "lint": "eslint src test",
    "prepublish": "npm run srcClean && npm run srcCompile",
    "srcClean": "rimraf ./lib",
    "srcCompile": "babel src --out-dir lib --source-maps",
    "test": "npm run coverage",
    "testClean": "rimraf ./lib-test ./coverage",
    "testCompile": "babel test --out-dir lib-test --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://github.com/wKovacs64/hibp",
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "node-fetch": "^1.5.3",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "es6-promise": "^3.2.1",
    "eslint": "^3.1.1",
    "eslint-config-semistandard": "^6.0.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "expect.js": "^0.3.1",
    "fetch-mock": "^5.0.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0",
    "rimraf": "^2.5.4"
  }
}
