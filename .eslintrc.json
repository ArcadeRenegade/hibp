{
  "extends": "airbnb",
  "rules": {
    "function-paren-newline": "off",
    "max-len": [
      "error",
      80,
      2
    ],
    "object-curly-newline": "off",
    "valid-jsdoc": [
      "error",
      {
        "prefer": {
          "arg": "param",
          "argument": "param",
          "return": "returns"
        }
      }
    ]
  }
}
